{
  "name": "axios-mock-server",
  "version": "0.16.3",
  "description": "RESTful mock server using axios",
  "author": "m-mitsuhide <m.mitsuhide@amatelus.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "bin": "bin/index.js",
  "homepage": "https://github.com/m-mitsuhide/axios-mock-server#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/m-mitsuhide/axios-mock-server.git"
  },
  "bugs": {
    "url": "https://github.com/m-mitsuhide/axios-mock-server/issues"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "axios",
    "test",
    "mock"
  ],
  "scripts": {
    "dev:cli": "ts-node src/lib/dev",
    "build": "rimraf dist && tsc --project tsconfig.build.json",
    "release": "standard-version --skip.tag",
    "lint": "eslint --ext .js,.ts --ignore-path .gitignore .",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test:types": "tsc --noEmit",
    "test:types:watch": "tsc --watch --noEmit"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/jest": "^25.2.1",
    "@types/minimist": "^1.2.0",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "axios": "^0.19.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "jest": "^25.4.0",
    "lint-staged": "^10.1.7",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "standard-version": "^7.1.0",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.9.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "chokidar": "^3.3.1",
    "minimist": "^1.2.5",
    "url-search-params-polyfill": "^8.0.0"
